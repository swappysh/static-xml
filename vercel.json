{
    "version": 2,
    "routes": [
        {
            "src": "/api/xml",
            "methods": [
                "POST"
            ],
            "dest": "/api/xml"
        }
    ],
    "functions": {
        "api/xml.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Credentials",
                    "value": "true"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "POST,OPTIONS"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "Content-Type"
                }
            ]
        }
    ]
}